{
  "version": 2,
  "builds": [
    {
      "src": "api/parse.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/parse",
      "dest": "api/parse.py"
    },
    {
      "src": "/",
      "dest": "frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "functions": {
    "api/parse.py": {
      "maxDuration": 30
    }
  }
}
